<lxs>
    <scene root="Root" axis_length="1"/>

    <views default="close">
        <perspective id="close" near="0.1" far="500" angle="45">
            <from x="30" y="15" z="30" />
            <to x="0" y="-2" z="0" />
        </perspective>
        <perspective id="far" near="0.3" far="550" angle="60">
            <from x="35" y="12" z="35" />
            <to x="0" y="-2" z="0" />
        </perspective>
        <ortho id="orthoCamera" near="0.1" far="500" left="-20" right="20" top="22" bottom="-22">
            <from x="6" y="5" z="30" />
            <to x="6" y="5" z="0" />
        </ortho>
    </views>

    <globals>
        <ambient r="0.2" g="0.2" b="0.2" a="1.0" />
        <background r="0.0" g="0.0" b="0.0" a="1.0" />
    </globals>

    <lights>
        <omni id="Light" enabled="1">
            <location x="9.8" y="11.9" z="10" w="1.0" />
            <ambient r="0" g="0" b="0" a="1.0" />
            <diffuse r="1.0" g="1.0" b="1.0" a="1.0" />
            <specular r="1.0" g="1.0" b="1.0" a="1.0" />
        </omni>
        <omni id="LightR" enabled="1">
            <location x="9.8" y="11.9" z="10" w="1.0" />
            <ambient r="0" g="0" b="0" a="1.0" />
            <diffuse r="1.0" g="0.0" b="0.0" a="1.0" />
            <specular r="1.0" g="0.0" b="0.0" a="1.0" />
        </omni>
        <omni id="LightG" enabled="1">
            <location x="9.8" y="11.9" z="10" w="1.0" />
            <ambient r="0" g="0" b="0" a="1.0" />
            <diffuse r="0.0" g="1.0" b="0.0" a="1.0" />
            <specular r="0.0" g="1.0" b="0.0" a="1.0" />
        </omni>
        <omni id="LightB" enabled="1">
            <location x="9.8" y="11.9" z="10" w="1.0" />
            <ambient r="0" g="0" b="0" a="1.0" />
            <diffuse r="0.0" g="0.0" b="1.0" a="1.0" />
            <specular r="0.0" g="0.0" b="1.0" a="1.0" />
        </omni>
    </lights>

    <textures>
        <texture id="tableWood" file="scenes/images/wood.png" />
        <texture id="metal" file="scenes/images/metal.png" />
        <texture id="pillow" file="scenes/images/pillow.png" />
        <texture id="floor" file="scenes/images/floor.png" />

    </textures>

    <materials>
        <material id="Material" shininess="10">
            <emission r="0.0" g="0.0" b="0.0" a="1.0" />
            <ambient r="1.0" g="1.0" b="1.0" a="1.0" />
            <diffuse r="0.6" g="0.0" b="0.0" a="1.0" />
            <specular r="0.6" g="0.0" b="0.0" a="1.0" />
        </material>
        <material id="white" shininess="1">
            <emission r="0" g="0" b="0" a="0" />
            <ambient r="1" g="1" b="1" a="1.0" />
            <diffuse r="1" g="1" b="1" a="1.0" />
            <specular r="1" g="1" b="1" a="1.0" />
        </material>
         <material id="cream" shininess="10">
            <emission r="0.0" g="0.0" b="0.0" a="1.0" />
            <ambient r="1.0" g="1.0" b="0.8" a="1.0" />
            <diffuse r="1.0" g="1.0" b="0.8" a="1.0" />
            <specular r="0.6" g="0.0" b="0.0" a="1.0" />
        </material>
        <material id="black" shininess="1">
            <emission r="0" g="0" b="0" a="0" />
            <ambient r="0" g="0" b="0" a="1.0" />
            <diffuse r="0" g="0" b="0" a="1.0" />
            <specular r="0.5" g="0.5" b="0.5" a="1.0" />
        </material>
    </materials>

    <transformations>
        <transformation id="Transform">
            <scale x="1" y="1" z="1" />
        </transformation>

        <transformation id="tableTransform">
            <scale x="1" y="1" z="1" /> 
        </transformation>

        <transformation id="tableTopTransform">
            <rotate axis="x" angle="-90" />
            <translate x="0" y="-5.5" z="2.4" />
            <scale x="6" y="6" z="1" /> 
        </transformation>

        <transformation id="tableLeg1Transform">
            <rotate axis="x" angle="-90" />
            <translate x="0.5" y="-5" z="0" />
        </transformation>

        <transformation id="tableLeg2Transform">
            <rotate axis="x" angle="-90" />
            <translate x="5.5" y="-5" z="0" />
        </transformation>

        <transformation id="tableLeg3Transform">
            <rotate axis="x" angle="-90" />
            <translate x="5.5" y="0" z="0" />
        </transformation>

        <transformation id="tableLeg4Transform">
            <rotate axis="x" angle="-90" />
            <translate x="0.5" y="0" z="0" />
        </transformation>

        <transformation id="lampTransform">
            <rotate axis="x" angle="-90" />
            <translate x="4.5" y="0" z="2.4" />
        </transformation>

        <transformation id="lampPostTransform">
            <translate x="0" y="0" z="0.1" />
        </transformation>

        <transformation id="lampTopTransform">
            <translate x="0" y="0" z="0.7" />
        </transformation>

        <transformation id="chair1Transform">
            <translate x="7" y="0" z="0" />
        </transformation>

        <transformation id="chair2Transform">
            <rotate axis="y" angle="-90" />
            <translate x="7" y="0" z="-6" />
        </transformation>

        <transformation id="chair3Transform">
            <translate x="7" y="0" z="-3" />
        </transformation>

        <transformation id="chair4Transform">
            <rotate axis="y" angle="-90" />
            <translate x="7" y="0" z="-9" />
        </transformation>

        <transformation id="chairLeg2Transform">
            <rotate axis="x" angle="-90" />
            <translate x="0.5" y="-4" z="0" />
        </transformation>

        <transformation id="chairLeg3Transform">
            <rotate axis="x" angle="-90" />
            <translate x="1.5" y="-4" z="0" />
        </transformation>

        <transformation id="chairLeg4Transform">
            <rotate axis="x" angle="-90" />
            <translate x="1.5" y="-5" z="0" />
        </transformation>

        <transformation id="chairSeatTransform">
            <rotate axis="x" angle="-90" />
            <translate x="0.2" y="-5.2" z="1.2" />
            <scale x="1.5" y="1.5" z="1" /> 
        </transformation>

        <transformation id="chairTopTransform">
            <translate x="1.7" y="1.1" z="4.5" />
            <rotate axis="y" angle="90" />
            <rotate axis="z" angle="45" />
        </transformation>

    </transformations>

    <animations>
        <animation id="crazy_mf">
            <keyframe instant="0">
                <translate x="0" y="0" z="0" />
                <rotate angle_x="0" angle_y="0" angle_z="0"/>
                <scale x="1" y="1" z="1"/>
            </keyframe>

            <keyframe instant="5">
                <translate x="3" y="3" z="3" />
                <rotate angle_x="123" angle_y="360" angle_z="179"/>
                <scale x="2.4" y="1.2" z="0.7"/>
            </keyframe>

            <keyframe instant="10">
                <translate x="0" y="0" z="0" />
                <rotate angle_x="0" angle_y="0" angle_z="0"/>
                <scale x="1" y="1" z="1"/>
            </keyframe>
        </animation>
    </animations>

    <primitives>
        <primitive id="floor">
            <rectangle x1="0" y1="0" x2="10" y2="10" />
        </primitive>
        <primitive id="Rectangle">
            <rectangle x1="0" y1="0" x2="1" y2="1" />
        </primitive>
        <primitive id="tableLeg">
            <cylinder base="0.1" top="0.1" height="2.4" slices="30" stacks="5" /> 
        </primitive>
        <primitive id="lampPost">
            <cylinder base="0.07" top="0.07" height="1" slices="30" stacks="5" /> 
        </primitive>
        <primitive id="lampBase">
            <cylinder base="0.3" top="0.01" height="0.2" slices="30" stacks="5" /> 
        </primitive>
        <primitive id="lampTop">
            <cylinder base="0.3" top="0" height="1" slices="30" stacks="5" /> 
        </primitive>
        <primitive id="Torus">
            <torus inner="0.03" outer="0.31" slices="30" loops="8" /> 
        </primitive>
        <primitive id="chairLeg">
            <cylinder base="0.05" top="0.05" height="1.2" slices="30" stacks="5" /> 
        </primitive>
        <primitive id="Triangle">
            <triangle x1="1.5" y1="0" z1="0" x2="0" y2="1.5" z2="0" x3="0" y3="0" z3="0"/>
        </primitive>
    </primitives>

    <components>

        <component id="tableLeg1">
            <transformation>
                <transformationref id="tableLeg1Transform"/>
            </transformation>
            <materials>
                <material id="Material" />
                <material id="white" />
                <material id="black" />
            </materials>
            <texture id="none" />
            <children>
                <primitiveref id="tableLeg" />
            </children>
        </component>

        <component id="tableLeg2">
            <transformation>
                <transformationref id="tableLeg2Transform"/>
            </transformation>
            <materials>
                <material id="Material" />
                <material id="white" />
                <material id="black" />
            </materials>
            <texture id="none" />
            <children>
                <primitiveref id="tableLeg" />
            </children>
        </component>

        <component id="tableLeg3">
            <transformation>
                <transformationref id="tableLeg3Transform"/>
            </transformation>
            <materials>
                <material id="Material" />
                <material id="white" />
                <material id="black" />
            </materials>
            <texture id="none" />
            <children>
                <primitiveref id="tableLeg" />
            </children>
        </component>

        <component id="tableLeg4">
            <transformation>
                <transformationref id="tableLeg4Transform"/>
            </transformation>
            <materials>
                <material id="Material" />
                <material id="white" />
                <material id="black" />
            </materials>
            <texture id="none" />
            <children>
                <primitiveref id="tableLeg" />
            </children>
        </component>

        <component id="tableTop">
            <transformation>
                <transformationref id="tableTopTransform"/>
            </transformation>
            <materials>
                <material id="white" />
            </materials>
            <texture id="tableWood" length_s="1" length_t="1" />
            <children>
                <primitiveref id="Rectangle" />
            </children>
        </component>

        <component id="table">
            <transformation>
                <transformationref id="tableTransform"/>
            </transformation>
            <animationref id="crazy_mf"/>
            <materials>
                <material id="Material" />
            </materials>
            <texture id="none" />
            <children>
                <componentref id="tableTop" />
                <componentref id="tableLeg1" />
                <componentref id="tableLeg2" />
                <componentref id="tableLeg3" />
                <componentref id="tableLeg4" />
            </children>
        </component>

        <component id="lampTop_0">
            <transformation>
                <transformationref id="lampTopTransform"/>
            </transformation>
            <materials>
                <material id="inherit" />
            </materials>
            <texture id="none" />
            <children>
                <primitiveref id="lampTop" />
                <primitiveref id="Torus" />
            </children>
        </component>

        <component id="lampPost_0">
            <transformation>
                <transformationref id="lampPostTransform"/>
            </transformation>
            <materials>
                <material id="inherit" />
                <material id="black" />
                <material id="Material" />
            </materials>
            <texture id="none" />
            <children>
                <primitiveref id="lampPost" />
            </children>
        </component>

        <component id="lamp">
            <transformation>
                <transformationref id="lampTransform"/>
            </transformation>
            <animationref id="crazy_mf"/>
            <materials>
                <material id="cream" />
            </materials>
            <texture id="none" />
            <children>
                <componentref id="lampPost_0" />
                <componentref id="lampTop_0" />
                <primitiveref id="lampBase" />
            </children>
        </component>

        <component id="chairLeg1">
            <transformation>
                <transformationref id="tableLeg1Transform"/>
            </transformation>
            <materials>
                <material id="inherit" />
                <material id="black" />
                <material id="Material" />
            </materials>
            <texture id="inherit" />
            <children>
                <primitiveref id="chairLeg" />
            </children>
        </component>

        <component id="chairLeg2">
            <transformation>
                <transformationref id="chairLeg2Transform"/>
            </transformation>
            <materials>
                <material id="inherit" />
                <material id="black" />
                <material id="Material" />
            </materials>
            <texture id="inherit" />
            <children>
                <primitiveref id="chairLeg" />
            </children>
        </component>

        <component id="chairLeg3">
            <transformation>
                <transformationref id="chairLeg3Transform"/>
            </transformation>
            <materials>
                <material id="inherit" />
                <material id="black" />
                <material id="Material" />
            </materials>
            <texture id="inherit" />
            <children>
                <primitiveref id="chairLeg" />
            </children>
        </component>

        <component id="chairLeg4">
            <transformation>
                <transformationref id="chairLeg4Transform"/>
            </transformation>
            <materials>
                <material id="inherit" />
                <material id="black" />
                <material id="Material" />
            </materials>
            <texture id="inherit" />
            <children>
                <primitiveref id="chairLeg" />
            </children>
        </component>

        <component id="chairSeat">
            <transformation>
                <transformationref id="chairSeatTransform"/>
            </transformation>
            <materials>
                <material id="white" />
            </materials>
            <texture id="pillow" length_s="1" length_t="1"/>
            <children>
                <primitiveref id="Rectangle" />
            </children>
        </component>

        <component id="chairTop">
            <transformation>
                <transformationref id="chairTopTransform"/>
            </transformation>
            <materials>
                <material id="white" />
            </materials>
            <texture id="pillow" length_s="1" length_t="1"/>
            <children>
                <primitiveref id="Triangle" />
            </children>
        </component>

        <component id="chair1">
            <transformation>
                <transformationref id="chair1Transform"/>
            </transformation>
            <animationref id="crazy_mf"/>
            <materials>
                <material id="Material" />
            </materials>
            <texture id="none" />
            <children>
                <componentref id="chair" />
            </children>
        </component>

        <component id="chair2">
            <transformation>
                <transformationref id="chair2Transform"/>
            </transformation>
            <materials>
                <material id="Material" />
            </materials>
            <texture id="none" />
            <children>
                <componentref id="chair" />
            </children>
        </component>

        <component id="chair3">
            <transformation>
                <transformationref id="chair3Transform"/>
            </transformation>
            <materials>
                <material id="Material" />
            </materials>
            <texture id="none" />
            <children>
                <componentref id="chair" />
            </children>
        </component>

        <component id="chair4">
            <transformation>
                <transformationref id="chair4Transform"/>
            </transformation>
            <materials>
                <material id="Material" />
            </materials>
            <texture id="none" />
            <children>
                <componentref id="chair" />
            </children>
        </component>

        <component id="chair">
            <transformation>
                <transformationref id="Transform"/>
            </transformation>
            <materials>
                <material id="white" />
            </materials>
            <texture id="metal" length_s="1" length_t="1" />
            <children>
                <componentref id="chairLeg1" />
                <componentref id="chairLeg2" />
                <componentref id="chairLeg3" />
                <componentref id="chairLeg4" />
                <componentref id="chairSeat" />
                <componentref id="chairTop" />
            </children>
        </component>

        <component id="floor">
            <transformation>
                <translate x="0" y="0" z="10" />
                <rotate axis="x" angle="-90" />
            </transformation>
            <materials>
                <material id="white" />
            </materials>
            <texture id="floor" length_s="0.4" length_t="0.4"/>
            <children>
                <primitiveref id="floor" />
            </children>
        </component>

        <component id="Root">
            <transformation>
                <transformationref id="Transform"/>
            </transformation>
            <materials>
                <material id="Material" />
            </materials>
            <texture id="none" />
            <children>
                <componentref id="table" />
                <componentref id="floor" />
                
                <componentref id="lamp" />
                <componentref id="chair1" />
                <componentref id="chair2" />
                <componentref id="chair3" />
                <componentref id="chair4" />
                
            </children>
        </component>
    </components>
    
</lxs>